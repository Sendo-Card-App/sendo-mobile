{
  "cli": {
    "version": ">= 15.0.15",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "environment": "development"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"  
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.sf-e.ca/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
